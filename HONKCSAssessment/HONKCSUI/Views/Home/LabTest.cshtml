
@{
    ViewBag.Title = "PracticalTest";
}
<style>
 .content-holder {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 20px 0;
}

 .text-div {
    margin-left: 10px;
  }
    #cover {
        background: #222 url('https://unsplash.it/1920/1080/?random') center center no-repeat;
        background-size: cover;
        height: 100%;
        text-align: center;
        display: flex;
        align-items: center;
        position: relative;
    }

    #cover-caption {
        width: 100%;
        position: relative;
        z-index: 1;
    }

    /* only used for background overlay not needed for centering */
    form:before {
        content: '';
        height: 100%;
        left: 0;
        position: absolute;
        top: 0;
        width: 100%;
        background-color: rgba(0,0,0,0.3);
        z-index: -1;
        border-radius: 10px;
    }
</style>
<h2>Lab Test</h2>
<div class="row">
    <form action="@Url.Action("SubmitStudent", "Students")" method="post">
        <p class="hint-text">Find Product Line and start lab test</p>
        <div class="autocomplete form-group" style="width:500px;">
            <input id="myInput" class="form-control" name="productline" type="text" placeholder="Product Line">
            <input type="submit">
        </div>
    </form>
